{
  "version": "17.0.0",
  "name": "@astronautlabs/emoji",
  "license": "MIT",
  "description": "A Unicode standard based way to implement emoji across all platforms.",
  "homepage": "https://github.com/astronautlabs/emoji",
  "keywords": [
    "emoji",
    "DOM",
    "parser",
    "images",
    "unicode"
  ],
  "devDependencies": {
    "@types/mustache": "^4.2.6",
    "@types/yaml": "^1.9.7",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Astronaut Labs LLC",
    "web": "https://astronautlabs.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/astronautlabs/emoji.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist.esm/index.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -b",
    "build:esm": "tsc -b tsconfig.esm.json",
    "clean": "rimraf dist dist.esm",
    "generate": "ts-node ./src/scripts/generate.ts",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "emoji-regex": "^10.6.0"
  }
}
